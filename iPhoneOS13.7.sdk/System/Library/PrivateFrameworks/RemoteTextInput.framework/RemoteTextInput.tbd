--- !tapi-tbd-v3
archs:                 [ armv7, armv7s, arm64, arm64e ]
platform:              ios
flags:                 [ flat_namespace ]
install-name:          /System/Library/PrivateFrameworks/RemoteTextInput.framework/RemoteTextInput
current-version:       1
compatibility-version: 1
objc-constraint:       retain_release
exports:
  - archs:                [ armv7, armv7s, arm64, arm64e ]
    symbols:              [ _RTICanLogMessageAtLevel, _RTILogFacility,
                            _RTIServiceAnonymousName, _RTIServiceIOSMacName,
                            _RTIServiceMediaRemoteName, _RTIServiceRapportName,
                            _RTIServiceSharingName, _RTIServiceSharingSafariName,
                            _RTIServiceTVControlServicesName,
                            _RemoteTextInputVersionNumber,
                            _RemoteTextInputVersionString,
                            __RTI_NSStringFromCGRect ]
    objc-classes:         [ RTIDataPayload, RTIDocumentState, RTIDocumentTraits,
                            RTIInputSystemClient, RTIInputSystemClientSession,
                            RTIInputSystemDataPayload, RTIInputSystemService,
                            RTIInputSystemServiceSession, RTIInputSystemSession,
                            RTIInputSystemSourceSession,
                            RTIStyledIntermediateText, RTITextOperations,
                            RTIUtilities ]
...
