--- !tapi-tbd-v3
archs:                 [ armv7, armv7s, arm64, arm64e ]
platform:              ios
flags:                 [ flat_namespace ]
install-name:          /System/Library/PrivateFrameworks/ControlCenterServices.framework/ControlCenterServices
current-version:       1
compatibility-version: 1
objc-constraint:       retain_release
exports:
  - archs:                [ armv7, armv7s, arm64, arm64e ]
    allowable-clients:    [ ConnectivityModule, ControlCenterServicesTests,
                            ControlCenterSettings, ControlCenterUI, CoverSheet,
                            HearingAidUIServer, HomeKitDaemon, LoginKit,
                            PerformanceTraceModule, SpringBoard, SpringBoardCore,
                            TVRemoteConnectionService,
                            com.apple.BarcodeSupport.Helper, controlcentertool,
                            logind ]
    symbols:              [ _CCSControlCenterServicesErrorDomain,
                            _CCSIsInternalInstall, _CCSLogModuleSettings,
                            _CCSLogRemoteService,
                            _CCSModuleVisibilityPreferenceDescription,
                            _CCSRegisterControlCenterLogging,
                            _CCSRemoteServiceName,
                            _CCSRemoteServiceServerInterface,
                            _CCSResolveModuleVisibility,
                            _CCSVisibilityPreferenceForBundleInfoDictionary,
                            _CCSVisibilityPreferenceForBundleProxy,
                            _ControlCenterServicesVersionNumber,
                            _ControlCenterServicesVersionString, _kVersionNumber ]
    objc-classes:         [ CCSControlCenterDefaults, CCSControlCenterService,
                            CCSModuleMetadata, CCSModulePresentationOptions,
                            CCSModuleRepository, CCSModuleSettingsProvider,
                            CCSRemoteServiceConnection, CCSRemoteServiceProvider,
                            CCSUsageMetrics ]
...
